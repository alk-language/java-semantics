gcd(a, b, out x, out y) {
    x = 1; y = 0; x1 = 0; y1 = 1; a1 = a; b1 = b;
    while (b1 > 0) {
        q = a1 / b1;
        tmp = x; x = x1; x1 = tmp - q * x1;
        tmp = y; y = y1; y1 = tmp - q * y1;
        tmp = a1; a1 = b1; b1 = tmp - q * b1;
    }
    return a1;
}
x = 0; y = 0; a = 55; b = 80;
g = gcd(a, b, x, y);
@assert a * x + b * y == g; // certification check