alg12(a: array<int>) : int
{
    lg = 1;
    i = 1;
    while (i < a.size())
    {
        if (a[i] == a[i - lg]) lg = lg+1;
        i = i + 1;
    }
    return lg;
}

symbolic $a: array<int>;
a = $a;
assume forall i:int :: 0 <= i && i < a.size()-1 ==> a[i] <= a[i+1];
assume a.size() == 3;

x = alg12(a);