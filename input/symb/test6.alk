@symbolic $a: array<int>;
a = $a;
@assume forall i:int :: 0 <= i && i < a.size()-1 ==> a[i] <= a[i+1];
@assume a.size() > 0;
slp = 1;
i = 1;
while (i < a.size())
@invariant 1 <= slp && i <= a.size();
//@invariant (exists k:int :: slp - 1 <= k && k < i && a[k] == a[k - slp + 1]);
@invariant (forall k:int :: slp <= k && k < i ==> a[k] != a[k - slp]);
@modifies slp;
@modifies i;
{
    if (a[i] == a[i - slp])
    {
        slp = slp + 1;
    }
    i = i + 1;
}