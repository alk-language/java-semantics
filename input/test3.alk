i = 0;
while (i < D.n) {
  p[i] = D.a[i];
  S[i] = 0;
  i = i + 1;
}
SB = emptyList;
SB.pushFront(i0);
S[i0] = 1;

while (SB.size() > 0) {
  i = SB.topFront();
  if (p[i].size() == 0) {
    SB.popFront();
  }
  else {
    j = p[i].topFront();
    p[i].popFront();
    if (S[j] == 0) {
      // visit j;
      S[j] = 1;
      SB.pushFront(j);
    }
  }
}
print(S);
