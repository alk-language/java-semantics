symbolic $a : array<int>, $i : int, $j : int;
a = [2, 4, 6, 8, 10];
b = a;
b[2] = $i;
assume $i == 6;
assert a == b;

c = [1, 3, 5, 7];
d = c;
d[2] = $j;
assume c == d;
assert $j == 5;
assert d.size() == 4;

symbolic $b : array<int>, $c : array<int>;
assume $b[0] == $c[0];
assume $b[1] == $c[1];
assume $b.size() == 2;
assume $c.size() == 2;
assert $b == $c;